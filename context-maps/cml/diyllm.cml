ContextMap DIYLLM {
    contains CustomerContext
	contains OpsContext
	contains TrainingContext

BoundedContext CustomerContext implements Marketing
BoundedContext OpsContext implements Ops
BoundedContext TrainingContext implements Training

}
Domain DIYLLMDomain {

	Subdomain Ops {
		type CORE_DOMAIN
	}

	Subdomain Training {
		type SUPPORTING_DOMAIN
	}

	Subdomain Monitoring {
		type GENERIC_SUBDOMAIN
	}

	Subdomain Feedback {
		type CORE_DOMAIN
	}

    Subdomain CustomerRelationship {
		type CORE_DOMAIN
	}

	Subdomain Marketing {
    	type SUPPORTING_DOMAIN
    }

    Subdomain Legal {
        type CORE_DOMAIN
    }

    Subdomain Accounting {
        type GENERIC_SUBDOMAIN
    }
}

